   Title={Vehicle Flow Detection Based on Improved Deep Structure and Deep Sort},
   Address = {Virtual-only Conference},
   author={Li, Haobin and Zhang, Yi},
   booktitle={2021 6th International Conference on Multimedia and Image Processing, {ICMIP'2021}},
